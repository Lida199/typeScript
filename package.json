    "@babel/preset-env": "^7.19.4",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.2",
    "eslint": "^8.26.0",
    "express": "^4.18.2",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.1.0",
    "localtunnel": "^2.0.2",
    "open": "^8.4.0",
    "path": "^0.12.7",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "typescript": "^4.9.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^5.3.3"
  }
  
