    "premessage": "node scripts/startMessage.js",
    "postmessage": "node scripts/endMessage.js",
    "build": "webpack --config webpack.config.js",
    "format:all": "prettier --write \"src/**/*.js\"",
    "lint": "eslint src"
  },
